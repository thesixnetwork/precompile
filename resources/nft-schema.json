          "boolean_attribute_value": {
            "value": false
          }
        },
        "data_type": "boolean",
        "required": true,
        "display_value_field": "value",
        "display_option": {
          "bool_true_value": "Yes",
          "bool_false_value": "No",
          "opensea": {
            "trait_type": "Claimed After Party Privileges"
          }
        },
        "hidden_overide": false,
        "hidden_to_marketplace": false
      },
      {
        "name": "check_alumni",
        "default_mint_value": {
          "boolean_attribute_value": {
            "value": false
          }
        },
        "data_type": "boolean",
        "required": true,
        "display_value_field": "value",
        "display_option": {
          "bool_true_value": "Yes",
          "bool_false_value": "No",
          "opensea": {
            "trait_type": "Is Alumni Checked"
          }
        },
        "hidden_overide": false,
        "hidden_to_marketplace": false
      }
    ],
    "actions": [
      {
        "name": "attend_main_stage",
        "desc": "Attend main stage event",
        "params": [],
        "when": "meta.GetBoolean('main_stage') == false",
        "then": [
          "meta.SetFloat('points_a', meta.GetFloat('points_a') + 100.0 )",
          "meta.SetBoolean('main_stage', true)",
          "meta.SetNumber('stage_count', meta.GetNumber('stage_count') + 1)"
        ]
      },
      {
        "name": "attend_stage",
        "desc": "Attend stage event",
        "params": [
          {
            "name": "stage",
            "desc": "Stage attribute",
            "data_type": "string",
            "required": true,
            "default_value": ""
          }
        ],
        "when": "meta.GetBoolean(params['stage'].GetString()) == false",
        "then": [
          "meta.SetFloat('points_a', meta.GetFloat('points_a') + 50.0 )",
          "meta.SetBoolean(params['stage'].GetString(), true)",
          "meta.SetNumber('stage_count', meta.GetNumber('stage_count') + 1)"
        ]
      },
      {
        "name": "attend_exhibition",
        "desc": "Attend partner exhibition",
        "params": [
          {
            "name": "exhibition",
            "desc": "exhibition attribute",
            "data_type": "string",
            "required": true,
            "default_value": ""
          }
        ],
        "when": "meta.GetBoolean(params['exhibition'].GetString()) == false",
        "then": [
          "meta.SetFloat('points_a', meta.GetFloat('points_a') + 20.0)",
          "meta.SetBoolean(params['exhibition'].GetString(), true)",
          "meta.SetNumber('exhibition_count', meta.GetNumber('exhibition_count') + 1)"
        ]
      },
      {
        "name": "attend_booth",
        "desc": "Attend to booth",
        "params": [
          {
            "name": "booth",
            "desc": "booth attribute",
            "data_type": "string",
            "required": true,
            "default_value": ""
          }
        ],
        "when": "meta.GetBoolean(params['booth'].GetString()) == false",
        "then": [
          "meta.SetFloat('points_a', meta.GetFloat('points_a') + 20.0)",
          "meta.SetBoolean(params['booth'].GetString(), true)",
          "meta.SetNumber('booth_count', meta.GetNumber('booth_count') + 1)"
        ]
      },
      {
        "name": "claim_extra_stage_point",
        "desc": "Claim extra stage point",
        "params": [],
        "when": "meta.GetNumber('stage_count') >= 5 && meta.GetBoolean('stage_extra_point') == false",
        "then": [
          "meta.SetFloat('points_a', meta.GetFloat('points_a') + 100.0)",
          "meta.SetBoolean('stage_extra_point', true)"
        ]
      },
      {
        "name": "claim_extra_exhibition_point",
        "desc": "Claim extra exhibition point",
        "params": [],
        "when": "meta.GetNumber('exhibition_count') >= 5 && meta.GetBoolean('exhibition_extra_point') == false",
        "then": [
          "meta.SetFloat('points_a', meta.GetFloat('points_a') + 100.0)",
          "meta.SetBoolean('exhibition_extra_point', true)"
        ]
      },
      {
        "name": "claim_extra_booth_point",
        "desc": "Claim extra booth point",
        "params": [],
        "when": "meta.GetNumber('booth_count') > 5 && meta.GetBoolean('booth_extra_point') == false",
        "then": [
          "meta.SetFloat('points_a', meta.GetFloat('points_a') + 150.0)",
          "meta.SetBoolean('booth_extra_point', true)"
        ]
      },
      {
        "name": "add_friend",
        "desc": "Add friend",
        "params": [
          {
            "name": "count",
            "desc": "amount for friend",
            "data_type": "number",
            "required": true,
            "default_value": ""
          }
        ],
        "when": "params['count'].GetNumber() > 0",
        "then": [
          "toAdd = params['count'].GetNumber() * 25.0",
          "meta.SetFloat('points_b', meta.GetFloat('points_b') + toAdd)",
          "meta.SetNumber('friend_count', meta.GetNumber('friend_count') + params['count'].GetNumber())"
        ]
      },
      {
        "name": "claim_friend_reach_10",
        "desc": "Claim extra reach 10 friend point",
        "params": [],
        "when": "meta.GetNumber('friend_count') >= 10 && meta.GetBoolean('friend_reach_10') == false",
        "then": [
          "meta.SetFloat('points_b', meta.GetFloat('points_b') + 50.0)",
          "meta.SetBoolean('friend_reach_10', true)"
        ]
      },
      {
        "name": "claim_friend_reach_20",
        "desc": "Claim extra reach 20 friend point",
        "params": [],
        "when": "meta.GetNumber('friend_count') >= 20 && meta.GetBoolean('friend_reach_20') == false",
        "then": [
          "meta.SetFloat('points_b', meta.GetFloat('points_b') + 100.0)",
          "meta.SetBoolean('friend_reach_20', true)"
        ]
      },
      {
        "name": "claim_friend_reach_30",
        "desc": "Claim extra reach 20 friend point",
        "params": [],
        "when": "meta.GetNumber('friend_count') >= 20 && meta.GetBoolean('friend_reach_30') == false",
        "then": [
          "meta.SetFloat('points_b', meta.GetFloat('points_b') + 150.0)",
          "meta.SetBoolean('friend_reach_30', true)"
        ]
      },
      {
        "name": "claim_unlimited_coffee",
        "desc": "Claim Unlimited Coffee",
        "params": [],
        "when": "meta.GetString('tier') != 'Staff'",
        "then": [
          "meta.SetNumber('unlimited_coffee', meta.GetNumber('unlimited_coffee') + 1)"
        ]
      },
      {
        "name": "redeem_prize",
        "desc": "redemption points for prize",
        "params": [
          {
            "name": "stock",
            "desc": "redemption stock",
            "data_type": "string",
            "required": true,
            "default_value": ""
          }
        ],
        "when": "meta.GetString('tier') != 'Staff' && meta.GetBoolean('redemption_used') == false && meta.GetFloat('points_a') >= meta.GetFloat(params['stock'].GetString() +'_a') && meta.GetFloat('points_b') >= meta.GetFloat(params['stock'].GetString() +'_b')",
        "then": [
          "meta.SetBoolean('redemption_used', true)",
          "meta.SetFloat('points_a', meta.GetFloat('points_a') - meta.GetFloat(params['stock'].GetString() +'_a'))",
          "meta.SetFloat('points_b', meta.GetFloat('points_b') - meta.GetFloat(params['stock'].GetString() +'_b'))",
          "meta.SetNumber(params['stock'].GetString() + '_stock', meta.GetNumber(params['stock'].GetString() + '_stock') + 1)"
        ]
      },
      {
        "name": "claim_after_party",
        "desc": "Claim After Party",
        "params": [],
        "when": "(meta.GetString('tier') == 'VIP' || meta.GetString('tier') == 'Speaker'|| meta.GetString('tier') == 'Investor') && meta.GetBoolean('after_party_claimed') == false || meta.GetString('tier') != 'Staff' && meta.GetBoolean('after_party_claimed') == false && meta.GetNumber('stage_count') >= 5 && meta.GetNumber('exhibition_count') >= 5 && meta.GetNumber('booth_count') >= 6 && meta.GetNumber('friend_count') >= 10",
        "then": [
          "meta.SetBoolean('after_party_claimed', true)",
          "meta.SetImage(meta.ReplaceAllString(meta.GetImage(),'normal','afterparty'))"
        ]
      },
      {
        "name": "verify_alumni",
        "desc": "verify user is alumni or not",
        "params": [
          {
            "name": "is_alumni",
            "desc": "true/false",
            "data_type": "number",
            "required": true,
            "default_value": ""
          }
        ],
        "when": "meta.GetBoolean('check_alumni') == false",
        "then": [
          "isTrue = (params['is_alumni'].GetNumber() > 0)",
          "meta.SetBoolean('alumni', isTrue)",
          "meta.SetImage(meta.ReplaceAllString(meta.GetImage(),'nonalumni','alumni'))",
          "meta.SetBoolean('check_alumni', true)"
        ]
      },
      {
        "name": "bypass_after_party",
        "desc": "By Pass After Party",
        "params": [],
        "when": "true",
        "then": [
          "meta.SetBoolean('after_party_claimed', true)",
          "meta.SetImage(meta.ReplaceAllString(meta.GetImage(),'normal','afterparty'))"
        ]
      },
      {
        "name": "change_price",
        "desc": "Change Price of redemption",
        "params": [
          {
            "name": "stock",
            "desc": "redemption stock",
            "data_type": "string",
            "required": true,
            "default_value": ""
          },
          {
            "name": "points_a",
            "desc": "New Hunt Point Value",
            "data_type": "number",
            "required": true,
            "default_value": ""
          },
          {
            "name": "points_b",
            "desc": "New Social Point Value",
            "data_type": "number",
            "required": true,
            "default_value": ""
          }
        ],
        "when": "true",
        "then": [
          "new_points_a = params['points_a'].GetNumber()",
          "new_points_b = params['points_b'].GetNumber()",
          "meta.SetFloat(params['stock'].GetString() +'_a', 0.0 + new_points_a)",
          "meta.SetFloat(params['stock'].GetString() +'_b', 0.0 + new_points_b)"
        ],
        "allowed_actioner": "ALLOWED_ACTIONER_SYSTEM_ONLY"
      }
    ]
  },
  "system_actioners": [
  ],
  "mint_authorization": "system"
}
